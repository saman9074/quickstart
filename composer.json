{
    "name": "saman9074/quickstart",
    "description": "A Laravel package to create a simple installer for your existing applications, similar to WordPress. / یک پکیج لاراول برای ایجاد یک نصب کننده ساده برای برنامه های موجود شما، شبیه به وردپرس.",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": ["laravel", "installer", "quickstart", "setup", "saman9074", "laravel10", "laravel11", "laravel12", "application installer"],
    "authors": [
        {
            "name": "Ali Abdi",
            "email": "abdi9074@gmail.com",
            "homepage": "https://github.com/saman9074",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Saman9074\\Quickstart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saman9074\\Quickstart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saman9074\\Quickstart\\QuickstartServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
